GET\",\n      headers: { \"Content-Type\": \"application/json\" },\n      // body: JSON.stringify({ user: { username: null } }),\n      credentials: \"include\",\n    })\n      .then((res) => res.json())\n      .then((json) => {\n        console.log(json);\n        // debugger;\n        if (json.status) throw json;\n      })\n      .catch((err) => alert(`${err.message}`));\n    setLogin(\"\");\n    setUserId(\"\");\n    setUsername(\"\");\n    setUserLookup(\"\");\n    // setFriendsFetch([]);\n    // setFriends([]);\n    // setRequestingYou([]);\n    // setYouRequested([]);\n    // setGroups([]);\n    setLogin(\"false\");\n  };\n\n  return (\n    <div className=\"App\">\n      <Router>\n        {/* link area is one of two conditions */}\n\n        {/* links that should always be available go here */}\n        <Link to=\"/\">\n          <h3>Home</h3>\n        </Link>\n\n        {login === \"false\" && (\n          <>\n            {/* <Link to=\"/login\" hidden=\"true\"> */}\n            <Link\n              to=\"/login\"\n              // hidden={localStorage.getItem(\"login\") === \"true\"}\n            >\n              <h3> Log In Here </h3>\n            </Link>\n            <Link\n              to=\"/signup\"\n              // hidden={localStorage.getItem(\"login\") === \"true\"}\n            >\n              <h3> New User? Sign Up Here </h3>\n            </Link>\n          </>\n        )}\n\n        {login === \"true\" && (\n          <>\n            <Link\n              to=\"/citizen\"\n              // hidden={localStorage.getItem(\"login\") !== \"true\"}\n            >\n              <h3>\n                Welcome to Invisible Cities, {username}. Click here to enter.\n              </h3>\n            </Link>\n            <Link\n              to=\"/logout\"\n              // hidden={localStorage.getItem(\"login\") !== \"true\"}\n            >\n              <h3>Logout here</h3>\n            </Link>\n          </>\n        )}\n\n        <div>\n          {/* no options selected, render nothing */}\n          {/* <Route path=\"/\" render={<></>} /> */}\n\n          {/* the components that render when login is false */}\n          <Route\n            path=\"/login\"\n            render={() => (\n              <Login\n                setUserId={setUserId}\n                setUsername={setUsername}\n                setLogin={setLogin}\n              />\n            )}\n          />\n          <Route path=\"/signup\" render={() => <UserForm />} />\n\n          {/* User has logged in and clicked enter.  Render the User Container here */}\n          <Route\n            path=\"/citizen\"\n            exact={true}\n            render={() => (\n              <UserContainer\n                setUserLookup={setUserLookup}\n                setFriendsFetch={setFriendsFetch}\n                setFriends={setFriends}\n                setRequestingYou={setRequestingYou}\n                setYouRequested={setYouRequested}\n                setGroups={setGroups}\n                setMessages={setMessages}\n              />\n            )}\n          />\n          <Route path=\"/logout\" render={() => logout()} />\n        </div>\n      </Router>\n    </div>\n  );\n}\n\nexport default App;\n",["121","122"],"/mnt/c/Users/ssats/Documents/GitHub/phase5/development/invisible-cities-frontend/src/components/ComposeMessage.js",[],"/mnt/c/Users/ssats/Documents/GitHub/phase5/development/invisible-cities-frontend/src/index.js",[],["123","124"],"/mnt/c/Users/ssats/Documents/GitHub/phase5/development/invisible-cities-frontend/src/components/Message.js",[],"/mnt/c/Users/ssats/Documents/GitHub/phase5/development/invisible-cities-frontend/src/containers/MessageBox.js",["125","126"],"/mnt/c/Users/ssats/Documents/GitHub/phase5/development/invisible-cities-frontend/src/containers/Friendlist.js",["127"],"/mnt/c/Users/ssats/Documents/GitHub/phase5/development/invisible-cities-frontend/src/containers/AllUsers.js",[],"/mnt/c/Users/ssats/Documents/GitHub/phase5/developmen